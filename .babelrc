{
	"presets": ["next/babel"],
	"plugins": [ [
		"styled-components",
		{ "ssr": true, "displayName": true, "preprocess": false }
	  ], [
		"module-resolver",
		{
		  "root": ["./"],
		  "alias": {
			"components": "./components",			
			"utils": "./utils",
			"views": "./views",
			"theme": "./theme",
			"lib": "./lib",
			"hooks": "./hooks"
		  }
		}
	  ]]
  }